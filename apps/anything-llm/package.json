{
  "name": "@decebal/anything-llm",
  "version": "1.0.0",
  "private": true,
  "description": "AnythingLLM RAG service for content generation",
  "scripts": {
    "start": "bun run src/server.js",
    "dev": "bun run src/server.js",
    "stop": "bun run src/stop.js",
    "restart": "bun run stop && bun run start",
    "status": "bun run src/status.js",
    "logs": "docker logs anythingllm-local -f",
    "shell": "docker exec -it anythingllm-local /bin/bash",
    "clean": "docker compose -f ../../docker-compose.anythingllm.yml down -v"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "dotenv": "^16.4.7"
  }
}
